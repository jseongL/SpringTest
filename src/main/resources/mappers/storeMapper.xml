<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.jsL.test.mybatisStore.repository.StoreRepository">
 	
 	<select id="selectStore" resultType="com.jsL.test.mybatisStore.domain.Store"> 
 	
 		SELECT
 			`name`
 			,`phoneNumber`
 			,`address`
 			,`businessNumber`
 			,`introduce`
 		FROM
 			`store`
 	
 	</select>
 
 
 </mapper>